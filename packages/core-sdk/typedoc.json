{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/_docs.ts"],
  "out": "docs/api",
  "readme": "none",
  "githubPages": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "includeVersion": false,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Config",
    "Actions",
    "Chains",
    "*"
  ],
  "groupOrder": [
    "Functions",
    "*"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/zama-ai/core-sdk",
    "npm": "https://www.npmjs.com/package/@zama-fhe/core-sdk"
  },
  "cleanOutputDir": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  }
}
