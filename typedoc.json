{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "githubPages": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Provider",
    "Hooks",
    "Config",
    "Chains",
    "Storage",
    "Types",
    "*"
  ],
  "groupOrder": [
    "Functions",
    "Classes",
    "Interfaces",
    "Type Aliases",
    "Variables",
    "*"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/zama-ai/react-sdk",
    "npm": "https://www.npmjs.com/package/@zama-fhe/sdk"
  },
  "cleanOutputDir": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  }
}
