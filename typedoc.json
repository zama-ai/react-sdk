{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Zama FHE SDK",
  "entryPoints": [
    "packages/core-sdk",
    "packages/react-sdk"
  ],
  "entryPointStrategy": "packages",
  "packageOptions": {
    "entryPoints": ["src/_docs.ts"]
  },
  "out": "docs/api",
  "readme": "README.md",
  "plugin": ["typedoc-plugin-markdown"],
  "outputFileStrategy": "members",
  "membersWithOwnFile": ["Function"],
  "fileExtension": ".md",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "excludeReferences": true,
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Hooks",
    "*"
  ],
  "groupOrder": [
    "Functions",
    "*"
  ],
  "cleanOutputDir": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  }
}
